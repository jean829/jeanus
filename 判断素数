#include <iostream>
#include <cmath>
using namespace std;
int isPrime(int a)
{
    int k;
if(a==2||a==3) k=1;
else
for(int i=2;i*i<=a;i++)
    if(a%2==0) {k=0;break;}
else k=1;
return k;
}


int main()
{
    unsigned short a;
    cin>>a;
    if (isPrime(a))
        cout<<"Y"<<endl;
    else
        cout<<"N"<<endl;
}
