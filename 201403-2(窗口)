#include <iostream>
using namespace std;
int a[12][6]={0},p[11]={0};
int main()
{
    int N,M,i,j,l,k=0;
    cin>>N>>M;//输入M，N
    l=M;
    for(i=0;i<N;i++)
    {
        for(j=0;j<4;j++)
        {
            cin>>a[i][j];
        }
        a[i][4]=i+1;
    }
    while(M--)
    {
        int x,y,s[5];
        cin>>x>>y;//输入每一行（共M行）
        for(i=N-1;i>=0;i--)
        {
            if((x>=a[i][0])&&(x<=a[i][2])&&(y>=a[i][1])&&(y<=a[i][3]))
            {
                { p[k]=a[i][4]; k++; }//记录第几个窗口
                //把该窗口的坐标位置记录一下
             for(j=0;j<5;j++)
                  s[j]=a[i][j];
                //重新排列窗口顺序
             for(int r=i;r<N-1;r++)
                {
                  a[r][0]=a[r+1][0];
                  a[r][1]=a[r+1][1];
                  a[r][2]=a[r+1][2];
                  a[r][3]=a[r+1][3];
                  a[r][4]=a[r+1][4];
                }
                //把刚刚点击的窗口设置现在最上层
             a[N-1][0]=s[0];
             a[N-1][1]=s[1];
             a[N-1][2]=s[2];
             a[N-1][3]=s[3];
             a[N-1][4]=s[4];
             break;
             }
             if(i==0)
             {
                 p[k]=-1;//没有找到该窗口就输出“IGNORED”，先设置为-1
                 k++;
             }
        }
    }
    //输出窗口位置
    for(i=0;i<l;i++)
    {
        if(p[i]>0) cout<<p[i];
        else cout<<"IGNORED";
        if(i!=l-1) cout<<endl;
    }
    return 0;
}
