#include <iostream>
using namespace std;
int main()
{
    int y,d,m,s;
    cin>>y>>d;
    if((y%4==0&&y%100!=0)||y%400==0)
    {
 //31 29 31 30 31 30 31 31 30 31 30 31
        if(d-31<=0) { m=1; s=d; }
        else if(d-31-29<=0) { m=2; s=d-31; }
        else if(d-31-29-31<=0) { m=3; s=d-31-29;}
        else if(d-31-29-31-30<=0) {m=4; s=d-31-29-31;}
        else if(d-31-29-31-30-31<=0) {m=5;s=d-31-29-31-30;}
        else if(d-31-29-31-30-31-30<=0) {m=6;s=d-31-29-31-30-31;}
        else if(d-31-29-31-30-31-30-31<=0) {m=7;s=d-31-29-31-30-31-30;}
        else if(d-31-29-31-30-31-30-31-31<=0) {m=8;s=d-31-29-31-30-31-30-31;}
        else if(d-31-29-31-30-31-30-31-31-30<=0) {m=9;s=d-31-29-31-30-31-30-31-31;}
        else if(d-31-29-31-30-31-30-31-31-30-31<=0) {m=10;s=d-31-29-31-30-31-30-31-31-30;}
        else if(d-31-29-31-30-31-30-31-31-30-31-30<=0) {m=11;s=d-31-29-31-30-31-30-31-31-30-31;}
        else {m=12;s=d-31-29-31-30-31-30-31-31-30-31-30;}
    }
    else
    {
 //31 28 31 30 31 30 31 31 30 31 30 31
        if(d-31<=0) { m=1; s=d; }
            else if(d-31-28<=0) { m=2; s=d-31; }
            else if(d-31-28-31<=0) { m=3; s=d-31-28;}
            else if(d-31-28-31-30<=0) {m=4; s=d-31-28-31;}
            else if(d-31-28-31-30-31<=0) {m=5;s=d-31-28-31-30;}
            else if(d-31-28-31-30-31-30<=0) {m=6;s=d-31-28-31-30-31;}
            else if(d-31-28-31-30-31-30-31<=0) {m=7;s=d-31-28-31-30-31-30;}
            else if(d-31-28-31-30-31-30-31-31<=0) {m=8;s=d-31-28-31-30-31-30-31;}
            else if(d-31-28-31-30-31-30-31-31-30<=0) {m=9;s=d-31-28-31-30-31-30-31-31;}
            else if(d-31-28-31-30-31-30-31-31-30-31<=0) {m=10;s=d-31-28-31-30-31-30-31-31-30;}
            else if(d-31-28-31-30-31-30-31-31-30-31-30<=0) {m=11;s=d-31-28-31-30-31-30-31-31-30-31;}
            else {m=12;s=d-31-28-31-30-31-30-31-31-30-31-30;}
    }
    cout<<m<<endl;
    cout<<s<<endl;
    return 0;
}
