#include <iostream>
using namespace std;
int main()
{
    int n,m;
    cin>>n;
    cin>>m;
    int a[32][32]={0},b[31][31]={0};
    for(int i = 1;i <= n;i++)
    {
        for(int j = 1;j <= m;j++)
        {
            cin >> a[i][j];
        }
    }
    for(int i = 1;i <= n;i++)
    {
        for(int j = 1;j <= m;j++)
        {
            if(a[i][j] == a[i][j-1] && a[i][j] == a[i][j+1] )
            {
                b[i][j] = 1;
                b[i][j-1] = 1;
                b[i][j+1] = 1;
            }
        }
    }
        for(int i = 1;i <= m;i++)
    {
        for(int j = 1;j <= n;j++)
        {
            if(a[j][i] == a[j-1][i] && a[j][i] == a[j+1][i] )
            {
                b[j][i] = 1;
                b[j-1][i] = 1;
                b[j+1][i] = 1;
            }
        }
    }
        for(int i = 1;i <= n;i++)
    {
        for(int j = 1;j <= m;j++)
        {
            if(b[i][j] == 1)
            {
                a[i][j] = 0;
            }
        }
    }
        for(int i = 1;i <= n;i++)
    {
        for(int j = 1;j <= m;j++)
        {
            if(j != m)
            {
                cout << a[i][j] << " ";
            }
            else
            {
                cout << a[i][j] << "\n";
            }
        }
    }
    return 0;
}
